{
    "name": "hasura-extra/metadata",
    "license": "MIT",
    "description": "The library help to manage Hasura metadata.",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Hasura\\Metadata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\Metadata\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/hasura-metadata"
    ],
    "require": {
        "php": ">=8.0",
        "symfony/console": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/string": "^5.0",
        "symfony/yaml": "^5.0",
        "hasura-extra/api-client": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
